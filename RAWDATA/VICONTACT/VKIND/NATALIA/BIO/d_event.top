    a: (^names()) $$intent=names $$sound_file = event1 Oleg.
    # age
    a: (^age()) $$intent=age $$sound_file = event2 Der ist 42 Jahre alt.
    # mood
    a: (^mood()) $$intent=mood $$sound_file = event3 Gut.
    # job
    a: (^job()) $$intent=job $$sound_file = event4 Der arbeitet auf der Baustelle.
    # relationship_f
    a: (^relationship_f()) $$intent=relationship_f Der ist der Freund von Mama.
    # relationship_o
    a: (^relationship_o()) $$intent=relationship_o $$sound_file = event5 Oleg ist doof.
    # time together
    a: (^time_together()) $$intent=time_together $$sound_file = event6 ^reuse(EVENT_2)
    # time relationship
    a: (^time()) $$intent=time_relationship $$sound_file = event7 Schon ganz lange.
    # activity together
    a: (^activity_together()) $$intent=activity_together ^refine()
        b: ($eventCounter==5) ^reuse(EVENT_6)
        b: () ^reuse(EVENT_6)
    # activity
    a: (^activity()) $$intent=activity $$sound_file = event9 Der arbeitet auf der Baustelle.
    # places together
    a: (^places_together()) $$intent=places_together $$sound_file = event10 Zu Hause.




outputmacro: ^emoHandler()


if ($chatPhase==main) {
	if ($eventCounter < 7) {$$emoType = serious}
	else if ($eventCounter < 10) {$$emoType = sad}
	else if ($eventCounter==10) {$$emoType = relieved}}
else if ($chatPhase==end){
	if ($eventCounter < 7) {$$emoType = disappointed}
	else if ($eventCounter < 10) {$$emoType = sad}
	else if ($eventCounter==10) {$$emoType = relieved}}



